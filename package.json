{
   "name": "typeorm-sample",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "mysql2": "^3.10.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "scripts": {
      "start": "ts-node -r tsconfig-paths/register src/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js ",
      "entity:create":"typeorm entity:create ",
      "subcriber:create":"typeorm subscriber:create ",
      "migration:create":"typeorm migration:create ",
      "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/ormconfig.ts migration:run",
      "migration:show": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource  src/config/ormconfig.ts migration:show",
      "query":"ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource  src/config/ormconfig.ts query "
   }
}